# OpenAI API Key for the LangGraph agent
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith API Key for tracing (optional)
# LANGSMITH_API_KEY=your_langsmith_api_key_here

# Required for production (Vercel) if you want to override the auto-detected URL
# REMOTE_AGENT_URL=https://your-app.vercel.app/api/agent

# Local LangGraph CLI development port (default is 8123)
# LANGGRAPH_DEPLOYMENT_URL=http://localhost:8123
